pragma solidity ^0.8.13;

import "hardhat/console.sol";
import { AccessControlUpgradeable } from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import { ERC721HolderUpgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";

import { CheloBase } from "./CheloBase.sol";
import { Proxiable } from "../proxy/Proxiable.sol";
import { ProxyWrapper } from "../proxy/ProxyWrapper.sol";
import { IChelo } from "../interfaces/core/IChelo.sol";
import { ILoanManager } from "../interfaces/core/ILoanManager.sol";
import { IAssetWrapper } from "../interfaces/core/IAssetWrapper.sol";
import { ILendingPool } from "../interfaces/core/ILendingPool.sol";
import { IERC20Decimals } from "../interfaces/utils/IERC20Decimals.sol";
import { ISettings } from "../interfaces/utils/ISettings.sol";
import { ILoanSignatureVerifier } from "../interfaces/utils/ILoanSignatureVerifier.sol";

contract LoanManager is ILoanManager, CheloBase, AccessControlUpgradeable, ERC721HolderUpgradeable, Proxiable {
    bytes32 public constant FACTORY_ROLE = keccak256("FACTORY_ROLE");
    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

    uint256 internal constant BASIS_POINTS = 10000;

    uint256 public defaultCount;
    address public owner;

    IChelo public protocolConfig;
    ILendingPool[] private _pools;
    mapping(ILendingPool => LoanMetadata[]) private _loans;

    constructor() {}

    function initialize(
        IChelo _protocolConfig,
        address _owner,
        address _manager
    ) external initializer {
        _grantRole(FACTORY_ROLE, msg.sender);
        _grantRole(OWNER_ROLE, _owner);
        _grantRole(MANAGER_ROLE, _owner);
        _grantRole(MANAGER_ROLE, _manager);
        _grantRole(DEFAULT_ADMIN_ROLE, _owner);

        owner = _owner;
        protocolConfig = _protocolConfig;
    }

    /// @inheritdoc ILoanManager
    function loan(ILendingPool pool, uint256 id) external view returns (LoanMetadata memory) {
        require(id < _loans[pool].length, "LoanManager:LOAN_DOESNT_EXIST");
        return _loans[pool][id];
    }

    /// @inheritdoc ILoanManager
    function getLoans(ILendingPool pool) external view returns (LoanMetadata[] memory) {
        return _loans[pool];
    }

    /// @inheritdoc ILoanManager
    function getPools() external view returns (ILendingPool[] memory) {
        return _pools;
    }

    /// @inheritdoc ILoanManager
    function getProtocolFee(uint256 amount) public view returns (uint256) {
        return (amount * protocolConfig.protocolFee()) / BASIS_POINTS;
    }

    function registerLendingPool(ILendingPool pool) public onlyRole(FACTORY_ROLE) {
        _pools.push(pool);
    }

    /// @inheritdoc ILoanManager
    function createLoan(
        uint256 principal,
        uint256 repayment,
        uint256 duration,
        uint256 poolId,
        address borrower
    ) external onlyRole(MANAGER_ROLE) returns (LoanMetadata memory loanData) {
        ILendingPool pool = _pools[poolId];

        loanData = LoanMetadata(
            LoanStatus.Issued,
            pool,
            false,
            0,
            principal,
            repayment,
            0,
            duration,
            block.timestamp + duration,
            borrower
        );

        _createLoan(loanData, pool);
    }

    /// @inheritdoc ILoanManager
    function createLoanWithCollateral(
        uint256 principal,
        uint256 repayment,
        uint256 duration,
        uint256 collateralId,
        uint256 poolId,
        address borrower
    ) external onlyRole(MANAGER_ROLE) returns (LoanMetadata memory loanData) {
        ILendingPool pool = _pools[poolId];
        IAssetWrapper assetWrapper = IAssetWrapper(protocolConfig.get(ASSET_WRAPPER));
        bytes memory parameters = assetWrapper.bundleMetadata(collateralId);

        loanData = LoanMetadata(
            LoanStatus.Issued,
            pool,
            true,
            collateralId,
            principal,
            repayment,
            0,
            duration,
            block.timestamp + duration,
            borrower
        );

        require(_isValidCollateralLoan(loanData, parameters), "LoanManager:wrong borrower parameters");

        assetWrapper.transferFrom(borrower, address(this), collateralId);
        _createLoan(loanData, pool);
    }

    /// @inheritdoc ILoanManager
    function repayLoan(
        ILendingPool pool,
        uint256 loanId,
        uint256 amount
    ) external {
        LoanMetadata storage loanData = _loans[pool][loanId];
        IERC20Decimals token = pool.token();
        IAssetWrapper assetWrapper = IAssetWrapper(protocolConfig.get(ASSET_WRAPPER));

        require(loanData.repaymentDate > 0, "LoanManager: loan doesnt exists");
        require(loanData.status != LoanStatus.Repaid, "LoanManager: loan already repaid");

        token.transferFrom(_msgSender(), address(loanData.pool), amount);

        loanData.amountRepaid += amount;
        if (loanData.amountRepaid >= loanData.repaymentAmount) {
            _setLoanStatus(pool, loanId, LoanStatus.Repaid);
            if (loanData.hasCollateral)
                assetWrapper.transferFrom(address(this), loanData.borrower, loanData.collateralId); //TODO in case of default try/catch
        }

        emit LoanPayment(pool, loanId, amount);
    }

    /// @inheritdoc ILoanManager
    function markLoanAsDefaulted(ILendingPool pool, uint256 loanId) external onlyRole(MANAGER_ROLE) {
        LoanMetadata memory loan = _loans[pool][loanId];
        IAssetWrapper assetWrapper = IAssetWrapper(protocolConfig.get(ASSET_WRAPPER));

        require(loan.status != LoanStatus.Repaid, "LoanManager: Loan already repaid");
        require(loan.repaymentDate <= block.timestamp, "LoanManager: Loan timestamp isnt finished");

        _setLoanStatus(pool, loanId, LoanStatus.Defaulted);
        if (loan.hasCollateral) {
            assetWrapper.safeTransferFrom(address(this), owner, loan.collateralId);
        }
        defaultCount++;
    }

    /// @inheritdoc ILoanManager
    function markLoanAsResolved(ILendingPool pool, uint256 loanId) external onlyRole(MANAGER_ROLE) {
        LoanMetadata memory loan = _loans[pool][loanId];
        require(loan.status == LoanStatus.Defaulted, "LoanManager: Loan is not defaulted");

        _setLoanStatus(pool, loanId, LoanStatus.Resolved);
    }

    function _createLoan(LoanMetadata memory loanData, ILendingPool pool) internal {
        require(_isValidLoan(loanData), "LoanManager: Invalid loan or state");
        require(loanData.principal > 0, "LoanManager: Invalid principal amount");
        require(loanData.repaymentAmount >= loanData.principal, "LoanManager: Invalid repayment amount");
        require(loanData.duration >= 1 days, "LoanManager: Invalid duration amount");

        uint256 protocolFee = getProtocolFee(loanData.principal);
        uint256 loanId = _loans[pool].length;
        uint256[] memory forwardAmounts = new uint256[](2);
        address[] memory forwardAddresses = new address[](2);

        forwardAddresses[0] = loanData.borrower;
        forwardAmounts[0] = loanData.principal - protocolFee;
        forwardAddresses[1] = protocolConfig.get(CHELO);
        forwardAmounts[1] = protocolFee;

        _loans[pool].push(loanData);
        pool.forward(forwardAddresses, forwardAmounts);

        emit LoanCreated(loanId, pool);
    }

    function _isValidLoan(LoanMetadata memory loan) private view returns (bool) {
        if (hasRole(OWNER_ROLE, msg.sender)) return true;
        ISettings settings = ISettings(protocolConfig.get(SETTINGS));

        return settings.isValidLoan(loan, this) && settings.isValidLoanManager(this);
    }

    function _isValidCollateralLoan(LoanMetadata memory loan, bytes memory borrowerParams) private view returns (bool) {
        (uint256 principalB, uint256 repaymentAmountB, uint256 durationB, address poolB) = abi.decode(
            borrowerParams,
            (uint256, uint256, uint256, address)
        );
        return
            loan.principal == principalB &&
            loan.repaymentAmount == repaymentAmountB &&
            loan.duration == durationB &&
            address(loan.pool) == poolB;
    }

    function _setLoanStatus(
        ILendingPool pool,
        uint256 loanId,
        LoanStatus newStatus
    ) internal {
        _loans[pool][loanId].status = newStatus;
        emit ChangedLoanStatus(pool, loanId, newStatus);
    }

    function _authorizeUpgrade(address) internal virtual override {
        revert("LoanManager:cannot upgrade");
    }
}

