pragma solidity ^0.8.13;

import { OwnableUpgradeable } from "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

import { CheloBase } from "./CheloBase.sol";
import { Proxiable } from "../proxy/Proxiable.sol";
import { IChelo } from "../interfaces/core/IChelo.sol";

contract Chelo is IChelo, CheloBase, OwnableUpgradeable, Proxiable {
    uint256 public protocolFee; // base 10000

    mapping(bytes32 => address) private _registry;

    constructor() {}

    /// @inheritdoc IChelo
    function initialize(address _protocolAddress, uint256 _protocolFee) external initializer {
        __Ownable_init();
        _registry[CHELO] = _protocolAddress;
        protocolFee = _protocolFee;
    }

    /// @inheritdoc IChelo
    function changeProtocolFee(uint256 _protocolFee) external onlyOwner {
        protocolFee = _protocolFee;
        emit ProtocolFeeChanged(protocolFee);
    }

    /// @inheritdoc IChelo
    function changeProtocolAddress(address _protocolAddress) external onlyOwner {
        _registry[CHELO] = _protocolAddress;
        emit ProtocolAddressChanged(_protocolAddress);
    }

    /// @inheritdoc IChelo
    function get(bytes32 name) external view returns (address) {
        return _registry[name];
    }

    /// @inheritdoc IChelo
    function set(bytes32 name, address element) external onlyOwner {
        _registry[name] = element;
    }

    function _authorizeUpgrade(address) internal virtual override onlyOwner {}
}

