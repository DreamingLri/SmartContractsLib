/// @title Chelo token funds custody contract. It manages deposited funds
/// @author Rafael C.
/// @dev deposits and returns gains to borrowers
interface ILendingPool {
    event Deposit(address indexed depositor, uint256 amount, uint256 shares);

    event Withdraw(address indexed withdrawer, uint256 shares, uint256 amount);

    event Forwarded(address indexed to, uint256 amount);

    function initialize(ILoanManager _loanManager, IERC20Decimals _token) external;

    function token() external view returns (IERC20Decimals);

    /// @dev deposits funds into the lending pool
    /// @param amount amount of tokens to deposit
    function deposit(uint256 amount) external;

    /// @dev withdraw funds from the lending pool
    /// @param shares amount of token shares to burn in exchange for pool tokens
    function withdraw(uint256 shares) external;

    /// @dev function that forwards funds from the lending pool to external accounts or contracts. This can only be called by the LoanManager
    /// @param targets addresses to send funds to
    /// @param amounts amounts to send to each address
    function forward(address[] memory targets, uint256[] memory amounts) external;

    /// @dev returns shares received by depositing x amount of tokens
    /// @param amount amount of tokens to deposit
    function sharesGivenAmount(uint256 amount) external view returns (uint256);

    /// @dev returns value of the pool (liquid value + illiquid value)
    function value() external view returns (uint256);

    /// @dev returns amount unpaid in the loans
    function unpaidAmount() external view returns (uint256);
}

