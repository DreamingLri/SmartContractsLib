pragma solidity ^0.8.13;

import { ILendingPool } from "./ILendingPool.sol";
import { ILoanManager } from "./ILoanManager.sol";
import { IChelo } from "./IChelo.sol";
import { IAssetWrapper } from "./IAssetWrapper.sol";
import { IERC20Decimals } from "../utils/IERC20Decimals.sol";

/// @title Chelo loan manager or portfolio. Stores and changes loan status, has rights to transfer and move funds from lending pools.
/// @author Rafael C.
interface ILoanManager {
    enum LoanStatus {
        Issued,
        Repaid,
        Defaulted,
        Resolved
    }

    struct LoanMetadata {
        LoanStatus status;
        ILendingPool pool;
        bool hasCollateral;
        uint256 collateralId;
        uint256 principal;
        uint256 repaymentAmount;
        uint256 amountRepaid;
        uint256 duration;
        uint256 repaymentDate;
        address borrower;
    }

    event LoanCreated(uint256 loanId, ILendingPool pool);

    event LoanPayment(ILendingPool pool, uint256 loanId, uint256 amount);

    event ChangedLoanStatus(ILendingPool pool, uint256 loanId, LoanStatus newStatus);

    event LendingPoolCreated(ILendingPool pool);

    function initialize(
        IChelo _protocolConfig,
        address manager,
        address _collateralReceiver
    ) external;

    function registerLendingPool(ILendingPool pool) external;

    /// @dev returns all pools created by the loan manager
    function defaultCount() external view returns (uint256);

    /// @dev returns all pools created by the loan manager
    function getPools() external view returns (ILendingPool[] memory);

    /// @dev returns all loans that used funds from a given pool
    /// @param pool pool where we want to query loans from
    function getLoans(ILendingPool pool) external view returns (ILoanManager.LoanMetadata[] memory);

    /// @dev returns the protocol fee given x amount of tokens
    function getProtocolFee(uint256 amount) external view returns (uint256);

    /// @dev returns the loan from a given pool and loanID
    function loan(ILendingPool pool, uint256 id) external view returns (LoanMetadata memory);

    /// @dev creates a loan without collateral
    /// @param principal the principal that the borrower will receive from the loan
    /// @param repayment the amount that the borrower must repay. It must be bigger than the principal amount
    /// @param duration the duration of the loan. It determines the limit date to repay the loan
    /// @param poolId the pool that will move funds for the borrower, its basically the token of the loan
    /// @param borrower the borrower address
    function createLoan(
        uint256 principal,
        uint256 repayment,
        uint256 duration,
        uint256 poolId,
        address borrower
    ) external returns (LoanMetadata memory);

    /// @dev creates a loan with collateral
    /// @param principal the principal that the borrower will receive from the loan
    /// @param repayment the amount that the borrower must repay. It must be bigger than the principal amount
    /// @param duration the duration of the loan. It determines the limit date to repay the loan
    /// @param collateralId the collateral token nft id (from the asset wrapper) that will be used as collateral
    /// @param poolId the pool that will move funds for the borrower, its basically the token of the loan
    /// @param borrower the borrower address
    function createLoanWithCollateral(
        uint256 principal,
        uint256 repayment,
        uint256 duration,
        uint256 collateralId,
        uint256 poolId,
        address borrower
    ) external returns (LoanMetadata memory loanData);

    /// @dev repays a loan. Transfer the tokens specified and updates the loan data
    /// @param pool lending pool address of the loan
    /// @param loanId the loan id created by the lending pool
    /// @param amount the amount to be repaid
    function repayLoan(
        ILendingPool pool,
        uint256 loanId,
        uint256 amount
    ) external;

    /// @dev marks a loan as defaulted. To be defaulted the date limit to pay the loan must have passed. This should only be called by the owner of the loan manager
    /// @param pool lending pool address of the loan
    /// @param loanId the loan id created by the lending pool
    function markLoanAsDefaulted(ILendingPool pool, uint256 loanId) external;

    /// @dev marks a loan as resolved. The 'resolved' status is used when a loan is defaulted and then the owner and borrower get into an agreement
    /// after it has been defaulted. Or the borrower repays the loan after it has defaulted
    /// @param pool lending pool address of the loan
    /// @param loanId the loan id created by the lending pool
    function markLoanAsResolved(ILendingPool pool, uint256 loanId) external;
}

