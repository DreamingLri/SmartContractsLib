pragma solidity ^0.8.13;

import "hardhat/console.sol";
import { AccessControlUpgradeable } from "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import { Chelo } from "./Chelo.sol";
import { LoanManager } from "./LoanManager.sol";
import { CheloBase } from "./CheloBase.sol";
import { ProxyWrapper } from "../proxy/ProxyWrapper.sol";
import { Proxiable } from "../proxy/Proxiable.sol";

import { ILendingPool } from "../interfaces/core/ILendingPool.sol";
import { ILoanManager } from "../interfaces/core/ILoanManager.sol";
import { IAssetWrapper } from "../interfaces/core/IAssetWrapper.sol";
import { IERC20Decimals } from "../interfaces/utils/IERC20Decimals.sol";
import { ICheloFactory } from "../interfaces/ICheloFactory.sol";

contract CheloFactory is ICheloFactory, CheloBase, AccessControlUpgradeable, Proxiable {
    bytes32 public constant FACTORY_MANAGER = keccak256("FACTORY_MANAGER");

    Chelo public protocolConfig;
    mapping(address => bool) public daoTemplate;

    modifier onlyTemplate() {
        require(_isTemplate(msg.sender), "CheloFactory: Caller is not dao template");
        _;
    }

    constructor() {}

    function initialize(Chelo _protocolConfig) external initializer {
        _grantRole(FACTORY_MANAGER, msg.sender);
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);

        protocolConfig = _protocolConfig;
    }

    /**
     * @dev cheloFactory template setter. Sets true for addresses that are aragon templates
     *
     * Requirements:
     *
     * - The caller must be FACTORY_MANAGER.
     * - templates and values must have the same length
     */
    function setTemplates(address[] memory templates, bool[] memory values) external onlyRole(FACTORY_MANAGER) {
        for (uint8 i = 0; i < templates.length; i++) daoTemplate[templates[i]] = values[i];
    }

    /**
     * @dev creates a loan manager given its owner and manager
     *
     * Requirements:
     *
     * - The caller must be a whitelisted template
     */
    function createLoanManager(address owner, address manager) public onlyTemplate returns (ILoanManager loanManager) {
        bytes memory initCalldata = abi.encodeWithSelector(
            ILoanManager.initialize.selector,
            protocolConfig,
            owner,
            manager
        );
        console.logBytes32(MANAGER_IMPLEMENTATION);
        loanManager = ILoanManager(address(new ProxyWrapper(protocolConfig.get(MANAGER_IMPLEMENTATION), initCalldata)));

        emit LoanManagerCreated(loanManager, owner, manager);

        return loanManager;
    }

    /**
     * @dev creates a lending pool
     *
     * Requirements:
     *
     * - The caller must have the MANAGER_ROLE or be a template
     */
    function createLendingPool(ILoanManager loanManager, IERC20Decimals token) public returns (ILendingPool) {
        require(
            LoanManager(address(loanManager)).hasRole(LoanManager(address(loanManager)).MANAGER_ROLE(), msg.sender) ||
                _isTemplate(msg.sender),
            "LendingPoolFactory: caller is not loanManager manager"
        );

        bytes memory initCalldata = abi.encodeWithSelector(
            ILendingPool.initialize.selector,
            address(loanManager),
            address(token)
        );

        ILendingPool pool = ILendingPool(
            address(new ProxyWrapper(protocolConfig.get(POOL_IMPLEMENTATION), initCalldata))
        );
        loanManager.registerLendingPool(pool);

        emit LendingPoolCreated(pool, loanManager);

        return pool;
    }

    function _isTemplate(address caller) private view returns (bool) {
        return daoTemplate[caller];
    }
}

