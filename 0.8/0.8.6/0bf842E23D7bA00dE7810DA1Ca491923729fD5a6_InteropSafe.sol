pragma solidity ^0.8.6;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";

import "./helpers.sol";

contract InteropSafe is ERC721Holder, Helpers {
    address internal _owner;
    uint256 public _nonce;

    event LogCast (
        uint256 indexed nonce,
        Execution[] executions,
        uint256 indexed chainId,
        uint256 gas,
        uint256 maxGasPrice,
        Assets[] assets,
        bytes metadata,
        bytes32 indexed interopTx
    );

    function setOwner (address owner_) public {
        require(_owner == address(0), "already-set");
        _owner = owner_;
    }

    function spell (InteropExecutionParams memory interopExecutionParams) internal {
        for (uint256 i = 0; i < interopExecutionParams.executions.length; i++) {
            Execution memory execution = interopExecutionParams.executions[i];
            if (execution.to == address(0)) continue; // TODO: should we revert or just skip it
            
            if (execution.callData.length == 0 && execution.value > 0) {
                // Send native token
                Address.sendValue(payable(execution.to), execution.value);
            } else {
                if (execution.operation == 0) {
                    // .call
                    Address.functionCallWithValue(
                        execution.to,
                        execution.callData,
                        execution.value
                    );
                } else if (execution.operation == 1) {
                    // .delegateCall
                    Address.functionDelegateCall(
                        execution.to,
                        execution.callData
                    );
                } else {
                    revert ("operation not found");
                }
            }
        }
    }

    function cast (
        bytes memory rlpData,
        uint256 txType,
        uint8 v,
        bytes32 r,
        bytes32 s,
        bytes32 interopTx
    ) external payable {
        bytes32 hash_ = keccak256(rlpData);
        address msgSender = ECDSA.recover(hash_, v, r, s);
        require(msgSender != address(0), "msgSender-not-valid");
        require(_owner == msgSender, "not-owner");

        InteropTxData memory interopTxData_ = decodeRLPTx(txType, rlpData);

        // TODO: check and add proper nonce checks.
        // TODO: refactor the condition?
        require(
            _nonce < interopTxData_.nonce || (interopTxData_.nonce == 0 && _nonce == 0), 
            "nonce-already-executed"
        );

        InteropExecutionParams memory interopExecutionParams = decodeExecutionCalldata(interopTxData_.data);
        
        spell(interopExecutionParams);

        _nonce = interopTxData_.nonce;

        // Emit Event
        emit LogCast(
            interopTxData_.nonce,
            interopExecutionParams.executions,
            interopExecutionParams.chainId,
            interopExecutionParams.gas,
            interopExecutionParams.maxGasPrice,
            interopExecutionParams.assets,
            interopExecutionParams.metadata,
            interopTx
        );
    }

    // TODO: add cast directly by user
    // TODO: add ERC721 onReceived function
    // TODO: deposit funds to IB contract


    receive() external payable {}
}

