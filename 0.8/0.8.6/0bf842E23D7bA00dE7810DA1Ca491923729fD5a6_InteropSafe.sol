pragma solidity ^0.8.6;
pragma experimental ABIEncoderV2;

import "../libs/RLPReader.sol";
import "../libs/BytesLib.sol";
import "../common/structs.sol";


contract Helpers is Structs {
    using RLPReader for RLPReader.RLPItem;
    using RLPReader for RLPReader.Iterator;
    using RLPReader for bytes;

    // TODO: move this to separate upgradable contract to reduce gas cost?
    function decodeRLPTx(uint256 txType, bytes memory rlpData) 
        public
        pure
        returns
    (InteropTxData memory interopTxData) {
        RLPReader.RLPItem[] memory rlpLsData = rlpData.toRlpItem().toList();

        if (txType == 0) { 
            // legacy tx
            interopTxData.nonce = rlpLsData[0].toUint();
            interopTxData.data = rlpLsData[5].toBytes();
        } else if (txType == 2) {
            // eip-1559 tx
            interopTxData.nonce = rlpLsData[1].toUint();
            interopTxData.data = rlpLsData[7].toBytes();
        }
    }
    
    // TODO: move this to separate upgradable contract to reduce gas cost?
    function decodeExecutionCalldata(bytes memory callData) 
    public
    pure
    returns (InteropExecutionParams memory interopExecutionParams) {
        // TODO: add check function sig condition
        bytes memory calldata_ = BytesLib.slice(callData, 5, callData.length - 4);
        (
           interopExecutionParams
        ) = abi.decode(calldata_, 
            (InteropExecutionParams)
        );
    }
}

