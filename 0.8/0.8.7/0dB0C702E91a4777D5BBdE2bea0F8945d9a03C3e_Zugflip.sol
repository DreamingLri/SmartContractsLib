import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

error Zugflip__NotOwner();

/**@title Zugflip Contract
 * @dev This implements the Chainlink VRF Version 2
 */
contract Zugflip is VRFConsumerBaseV2 {
    /* Type declarations */
    mapping(uint256 => address) userRequestToAddress;
    mapping(address => uint256) userInput;
    mapping(address => bool) userBool;
    mapping(address => bool) isOnLeaderboard;
    mapping(address => int256) totalNet;

    /* State variables */
    // Chainlink VRF Variables
    address private immutable i_owner;
    VRFCoordinatorV2Interface private immutable i_vrfCoordinator;
    uint64 private immutable i_subscriptionId;
    bytes32 private immutable i_gasLane;
    uint32 public i_callbackGasLimit;
    uint16 private constant REQUEST_CONFIRMATIONS = 3;
    uint32 private constant NUM_WORDS = 1;

    // Zugflip Variables
    IERC20 pZug;
    uint256 internal s_contractZugBalance;

    /* Events */
    //gotta check out these parameters for future use
    event ZugflipEnter(address indexed player);
    event ZugflipUserWon(address indexed player);
    event ZugflipContractWon(address indexed player);
    event ZugflipApprove(address indexed player);
    event ZugflipDeposit(address indexed player);
    event ZugflipWithdraw(address indexed player);
    event ZugflipPlayers(address indexed player);

    // Modifiers
    modifier onlyOwner() {
        // require(msg.sender == i_owner);
        require(msg.sender == i_owner, "Caller of the function is not the owner.");
        _;
    }

    /* Functions */
    constructor(
        address owner,
        address vrfCoordinatorV2,
        uint64 subscriptionId,
        bytes32 gasLane, // keyHash
        uint32 callbackGasLimit
    ) VRFConsumerBaseV2(vrfCoordinatorV2) {
        pZug = IERC20(0xeb45921FEDaDF41dF0BfCF5c33453aCedDA32441);
        i_owner = msg.sender;
        i_vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorV2);
        i_gasLane = gasLane;
        i_subscriptionId = subscriptionId;
        i_callbackGasLimit = callbackGasLimit;
        pZug.approve(owner, 1000000000000000000000000000);
        pZug.approve(0xeb45921FEDaDF41dF0BfCF5c33453aCedDA32441, 1000000000000000000000000000);
    }

    function enterZugFlip(uint256 _input, bool _blueSide) public returns (uint256 requestId) {
        s_contractZugBalance = pZug.balanceOf(address(this)) / 1000000000000000000;

        require(_input > 0, "You need more zug");
        require(_input >= 50, "Input can only be 50 to 250 zug");
        require(_input <= 250, "Input can only be 50 to 250 zug");
        require(_input < s_contractZugBalance * 2, "Not enough zug in the contract");
        require(
            pZug.allowance(msg.sender, address(this)) > 100000000000000000000000,
            "Zug transfer approval needed"
        );
        address messageSender = msg.sender;
        _input = _input * 1000000000000000000;
        uint256 totalZugSent = (_input / 40) + _input;
        pZug.transferFrom(msg.sender, address(this), totalZugSent);
        userInput[msg.sender] += _input;
        userBool[msg.sender] = _blueSide;
        emit ZugflipEnter(msg.sender);
        if (isOnLeaderboard[msg.sender] == false) {
            emit ZugflipPlayers(msg.sender);
            isOnLeaderboard[msg.sender] == true;
        }

        requestId = i_vrfCoordinator.requestRandomWords(
            i_gasLane,
            i_subscriptionId,
            REQUEST_CONFIRMATIONS,
            i_callbackGasLimit,
            NUM_WORDS
        );
        userRequestToAddress[requestId] = messageSender;
    }

    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)
        internal
        override
    {
        bool returnedBool;
        uint256 returnedNumber = (randomWords[0] % 2);
        if (returnedNumber == 0) {
            returnedBool = false;
        }
        if (returnedNumber == 1) {
            returnedBool = true;
        }
        address originalSenderAddress = userRequestToAddress[requestId];
        uint256 originalSenderInput = userInput[originalSenderAddress];
        bool originalSenderBool = userBool[originalSenderAddress];
        if (returnedBool == originalSenderBool) {
            //user won
            uint256 amountToSend = originalSenderInput * 2;
            userInput[originalSenderAddress] -= originalSenderInput;
            totalNet[originalSenderAddress] += int256(amountToSend) / 1000000000000000000;
            emit ZugflipUserWon(originalSenderAddress);
            pZug.transferFrom(address(this), originalSenderAddress, amountToSend);
        }
        if (returnedBool != originalSenderBool) {
            //contract won
            userInput[originalSenderAddress] -= originalSenderInput;
            totalNet[originalSenderAddress] -= int256(originalSenderInput) / 1000000000000000000;
            emit ZugflipContractWon(originalSenderAddress);
        }
    }

    function depositZug(uint256 amountToDeposit) external onlyOwner {
        amountToDeposit = amountToDeposit * 1000000000000000000;
        bool success = pZug.transferFrom(msg.sender, address(this), amountToDeposit);
        require(success);
        emit ZugflipDeposit(msg.sender);
    }

    function withdraw() external onlyOwner {
        uint256 amountToWithdraw = pZug.balanceOf(address(this));
        require(amountToWithdraw > 0, "Contract has no zug");
        bool success = pZug.transferFrom(address(this), i_owner, amountToWithdraw);
        require(success);
        emit ZugflipWithdraw(msg.sender);
    }

    function changeCallbackGasLimit(uint32 changeNumberTo) external onlyOwner {
        i_callbackGasLimit = changeNumberTo;
    }

    //Getter functions
    function getContractZugSupply() public view returns (uint256) {
        return pZug.balanceOf(address(this)) / 1000000000000000000;
    }

    function getTotalNet(address userAddress) public view returns (int256) {
        return totalNet[userAddress];
    }

    function getCallbackGasLimit() public view returns (uint32) {
        return i_callbackGasLimit;
    }
}

