import "../libs/LibAccess.sol";
import "../libs/LibStorage.sol";
import "../common/AccessTypes.sol";


import "hardhat/console.sol";


abstract contract BaseVault {
    using LibAccess for AccessTypes.AccessControl;
    
    function _msgSender() internal view returns (address payable) {
        return payable(msg.sender);
    }

    function hasRole(bytes32 role, address actor) public view returns (bool) {
        return LibStorage.getAccessStorage().hasRole(role, actor);
    }

    function isAdmin(address actor) public view returns (bool) {
        return LibStorage.getAccessStorage().hasRole(AccessTypes.ADMIN_ROLE, actor);
    }

    function isDelegate(address actor) public view returns (bool) {
        return LibStorage.getAccessStorage().hasRole(AccessTypes.DELEGATE_ROLE, actor);
    }

    modifier onlyAdmin() {
        require(hasRole(AccessTypes.ADMIN_ROLE, _msgSender()), "Not admin");
        _;
    }

    modifier onlyAuthorized() {
        require(hasRole(AccessTypes.ADMIN_ROLE, _msgSender()) || 
                hasRole(AccessTypes.DELEGATE_ROLE, _msgSender()), "Not authorized");
        _;
    }

    modifier onlyDelegate() {
        require(hasRole(AccessTypes.DELEGATE_ROLE, _msgSender()), "Only delegates allowed");
        _;
    }

    modifier initializer() {
        InitTypes.InitControls storage ic = LibStorage.getInitControls();
        require(ic.initializing || !ic.initialized, "Already initialized");
        
        bool tlc = !ic.initializing;
        if(tlc) {
            ic.initializing = true;
            ic.initialized = true;
        }
        _;
        if(tlc) {
            ic.initializing = false;
        }
    }

    modifier nonReentrant() {
        
        require(!LibStorage.getAccessStorage().reentrantFlag, "Attempting to re-enter function recursively");
        LibStorage.getAccessStorage().reentrantFlag = true;
        _;
        LibStorage.getAccessStorage().reentrantFlag = false;
    }

    modifier whenNotPaused() {
        require(!LibStorage.getAccessStorage().paused, "Contract paused");
        _;
    }

    //================ MUTATIONS ===============/

    function pause() public onlyAdmin {
        LibStorage.getAccessStorage().paused = true;
    }

    function resume() public onlyAdmin {
        LibStorage.getAccessStorage().paused = false;
    }
    
    function addAdmin(address actor) public onlyAdmin {
       LibStorage.getAccessStorage().addAdmin(actor);
    }

    function addAdmins(address[] calldata actors) public onlyAdmin {
        for(uint i=0;i<actors.length;++i) {
            addAdmin(actors[i]);
        }
    }

    function removeAdmin(address actor) public onlyAdmin {
        LibStorage.getAccessStorage().removeAdmin(actor);
    }

    function addDelegate(address actor) public onlyAdmin {
       LibStorage.getAccessStorage().addDelegate(actor);
    }

    function removeDelegate(address actor) public onlyAdmin {
        LibStorage.getAccessStorage().removeDelegate(actor);
    }

    function addRole(bytes32 role, address actor) public onlyAdmin {
        _setupRole(role, actor);
    }

    function revokeRole(bytes32 role, address actor) public onlyAdmin {
        LibStorage.getAccessStorage()._revokeRole(role, actor);
    }

    function _setupRole(bytes32 role, address actor) internal {
        LibStorage.getAccessStorage()._addRole(role, actor);
    }

    function _initAccess(address o) internal initializer {
        _setupRole(AccessTypes.ADMIN_ROLE, o);
    }
}

