import "@openzeppelin/contracts/utils/Strings.sol";

contract StrategyVault is BaseVault {

    using SafeERC20 for IERC20;
    using LibFees for FeeTypes.FeeStorage;

    struct State {
        bool completed;
        string failedReason;
        uint step;
    }

    //emitted when native funds are sent to vault to pay for gas fees
    event FundingReceived(address indexed sender, uint amount);

    //emitted when the vault is running low on gas fees. Monitoring solutions
    //can monitor for this event to know when to bump up gas fees in the vault.
    event FundingLow(address indexed vault, uint remaining);

    //emitted when the vault pays gas fees
    event GasFeesPaid(address indexed receiver, uint amount);

    //emitted when a strategy executes one or more of its steps
    event StrategySuccess(address indexed vault, bytes32 indexed merkleyRoot, uint upToStep);

    //emitted when a strategy fails to complete
    event StrategyFailed(address indexed vault, bytes32 indexed merkleRoot, string reason);

    function initialize(address owner, uint112 lowBalThreshold) public initializer {
        BaseVault._initAccess(owner);
        AccessTypes.AccessControl storage acs = LibStorage.getAccessStorage();
        //this should be initialized by the delegate responsible for invoking 
        //strategies through the vault.
        acs.roles[AccessTypes.DELEGATE_ROLE][_msgSender()] = true;
        LibStorage.getVaultStorage().lowBalanceThreshold = lowBalThreshold;
    }

    //used to receive native gas fees
    receive() external payable {
        emit FundingReceived(_msgSender(), msg.value);
    }

    function execute(StrategyTypes.StrategyRequest calldata request) public onlyDelegate returns (bool success, string memory fail) {
       
        /*
        bytes memory res;
        (success, res) = target.delegatecall(data);
        if(!success) {
            return (false, "Failed to call target");
        }
        (success, fail) = abi.decode(res, (bool, string));
        */
        
        (success, fail) = _execSteps(request);
        // require(success, fail);
        if(!success) {
            LibStorage.getFeeStorage()._clearFees();
            revert(fail);
            // return (success, fail);
        }
        address dexible = IDexibleExecutor(_msgSender()).getDevTeam();
        LibStorage.getFeeStorage()._distributeFees(request.fees.affiliate, 
                                                          dexible,
                                                         request.fees.affiliatePortion);
        return(true, "");
    }

    function withdraw(IERC20 token, address receiver, uint amount) public onlyAdmin {
        token.safeTransfer(receiver, amount);
    }

    function payGas(address receiver, uint amount) public onlyDelegate returns (bool success, string memory fail) {
        uint bal = address(this).balance;
        if(bal < amount) {
            return (false, "Insufficient funds to pay gas");
        }

        if(bal <= LibStorage.getVaultStorage().lowBalanceThreshold) {
            emit FundingLow(address(this), bal);
        }
        
        if(!payable(receiver).send(amount)) {
            return (false, "Failed to pay gas fees");
        }
        emit GasFeesPaid(receiver, amount);
        return (true, "");
    }

    function _execSteps(StrategyTypes.StrategyRequest calldata request) internal returns (bool success, string memory fail) {
       
        IDexibleExecutor exec = IDexibleExecutor(_msgSender());
        FeeTypes.FeeStorage storage fs = LibStorage.getFeeStorage();
        VaultTypes.VaultStorage storage vs = LibStorage.getVaultStorage();
        bytes32 root = request.auth.merkleRoot;

        State memory state;
        state.completed = true;
        state.failedReason = "";
        for(uint i=0;i<request.steps.length;++i) {

            state.step = i;
            
            StrategyTypes.StrategyStep calldata s = request.steps[i];
            console.log("Executing strategy step", i);
            console.log("module:", s.module);
            if(!exec.allowedModule(s.module)) {
                console.log("Module is not allowed");
                state.failedReason = "Module is not allowed";
                state.completed = false;
                break;
                // revert("Module call not approved");
            }

            if(s.index == 1) {
                //make sure no root clashes
                if (vs.completedSteps[root] != 0) {
                    console.log("Merkel root clash");
                    state.failedReason = "Merkel root clash";
                    state.completed = false;
                    break;
                }
                // require(vs.completedSteps[root] == 0, "Merkel root clash");
            }
            
            if (vs.completedSteps[root] >= s.index) {
                console.log("Already executed step for this strategy");
                state.failedReason = "Already executed step for this strategy";
                state.completed = false;
                break;
            }
            // require(vs.completedSteps[root] < s.index, "Already exected step for this strategy");
            
            console.log("Getting fee for module");
            //uint8 minBps = exec.feeForModule(s.module);
            (uint8 minBps, uint8 maxBps) = exec.feeRangeForModule(s.module);
            if (request.fees.bps < minBps) {
                console.log("Bps fees did not meet minimum threshold");
                state.failedReason = "Bps fees did not meet minimum threshold";
                state.completed = false;
                break;
            }
            // require(request.fees.bps >= minBps, "Bps fees did not meet minimum threshold");
            
            if (request.fees.bps > maxBps) {
                console.log("Bps fee exceeds max allowed threshold");
                state.failedReason = "Bps fee exceeds max allowed threshold";
                state.completed = false;
                break;
            }
            // require(request.fees.bps <= maxBps, "Bps fee exceeds max allowed threshold");

            fs._tempBps = request.fees.bps;
            console.log("Fee for module", fs._tempBps);
            bytes memory invoke = abi.encodeWithSelector(IPlatform.execute.selector, request, i);
            bytes memory res;
            console.log("D-Call module", request.steps[i].module);
            (success, res) = request.steps[i].module.delegatecall(invoke);
            if(!success) {
                // emit StrategyFailed(address(this), request.auth.merkleRoot, "Failed to call module");
                // return (false, "Failed to call module");
                console.log("Failed to call module");
                state.failedReason = "Failed to call module";
                state.completed = false;
                break;
            }
            (success, fail) = abi.decode(res, (bool, string));
            if(!success) {
                // emit StrategyFailed(address(this), request.auth.merkleRoot, fail);
                // return (success, fail);
                console.log(fail);
                state.failedReason = fail;
                state.completed = false;
                break;
            }
        }
        vs.completedSteps[root] = request.steps[request.steps.length-1].index;
        // emit StrategySuccess(address(this), request.auth.merkleRoot, request.steps[request.steps.length-1].index);
        
        if (!state.completed) {
            state.failedReason = string(abi.encodePacked(state.failedReason, " at step:", Strings.toString(state.step)));
        }

        // return (true, "");
        return (state.completed, state.failedReason);
    }
}

