pragma solidity ^0.8.10; // ok 

import "./MultiSignWallet.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import '@openzeppelin/contracts/access/Ownable.sol';
import "@openzeppelin/contracts/security/ReentrancyGuard.sol"; // ok
import '@openzeppelin/contracts/utils/structs/EnumerableSet.sol';  // ok
import "hardhat/console.sol";

contract MultiSignWalletFactory is Ownable, Pausable, ReentrancyGuard{  
    using EnumerableSet for EnumerableSet.AddressSet; // OK
    EnumerableSet.AddressSet private wallets;
    address  public last_created_wallet;

    mapping(address => EnumerableSet.AddressSet) private OwnerToWallets;

    modifier walletOnly { // ok
    require(wallets.contains(msg.sender), 'caller not a Wallet'); // ok
    _; // o
  }

    event MultiSignWalletCreated(address waletAdress, address creator, address[] owners, uint q);

     function createMultiSignWallet(address[] memory owners, uint q) external whenNotPaused {
        require(owners.length >= q, 'quorum less than owner length');
       MultiSignWallet wallet = new MultiSignWallet(owners, q);
       wallets.add(address(wallet));
       last_created_wallet = address(wallet);
       emit MultiSignWalletCreated(address(wallet), msg.sender, owners, q);
       for (uint i; i < owners.length; i++) {
           OwnerToWallets[owners[i]].add(address(wallet));

       }
     }

     function updateOwner(address own, bool isTrusted) external walletOnly nonReentrant{
         if (isTrusted) {
            OwnerToWallets[own].add(address(msg.sender));
        } else {
            OwnerToWallets[own].remove(address(msg.sender));
        }
     }

     function getAllWallets() external view returns (address[] memory) { 
		address[] memory list = new address[](wallets.length());
        console.log(wallets.length());
		for (uint i; i < wallets.length(); i++) { // ok
        console.log(wallets.at(i));
        list[i] = wallets.at(i); // ok
		} // ok
		return list; // ok
	} // 
    
    function getWallets(address a) external view returns (address[] memory) { 
		address[] memory list= new address[](OwnerToWallets[a].length());
		for (uint i; i < OwnerToWallets[a].length(); i++) { // ok
        list[i] = OwnerToWallets[a].at(i); // ok
		} // ok
		return list; // ok
	} // o

     function setPause(bool pause) external onlyOwner {
        if (pause) {
            _pause();
        } else {
            _unpause();
        }
    }
}

