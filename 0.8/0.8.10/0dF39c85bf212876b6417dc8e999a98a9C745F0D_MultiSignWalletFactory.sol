pragma solidity ^0.8.10; // ok 

import '@openzeppelin/contracts/token/ERC20/IERC20.sol'; // OK
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol'; // OK
import '@openzeppelin/contracts/utils/Address.sol'; // OK
import '@openzeppelin/contracts/utils/structs/EnumerableSet.sol';  // ok
import "@openzeppelin/contracts/security/ReentrancyGuard.sol"; // ok
import "hardhat/console.sol";

interface FactoryStub {
  function updateOwner(address own, bool isTrusted) external;
}

contract MultiSignWallet is ReentrancyGuard  {  // ok
  using SafeERC20 for IERC20; // OK
  using EnumerableSet for EnumerableSet.UintSet; // OK
  using EnumerableSet for EnumerableSet.AddressSet; // OK

  struct RequestMetadata {
    uint id;
    address initiator;
    bool confirmed;
    uint timestamp;
    uint numOfConfirmations;
    uint numOfDeclines;
  }
  struct TrustedRequest {
    RequestMetadata m;
    address user;
    bool isTrusted;
  }
  struct QuorumRequest {
    RequestMetadata m;
    uint quorum;
  }
  struct Request {
    RequestMetadata m;
    uint amount;
    IERC20 token;
  }

  address public factory;
  uint public quorum;

  EnumerableSet.AddressSet private trustedSet;
  mapping(address => bool) public trusted; // Everyone that is trusted, should likely be an enumerableSet.AddressSet! 
  
  modifier trustedOnly { // ok
    require(trusted[msg.sender], 'not trusted'); // ok
    _; // ok
  }

  mapping(uint => TrustedRequest) public trRequests;
  EnumerableSet.UintSet private trRequestList; // NOTE[SELF]| The index of trRequestList does not necessarily equal the trRequests index!
  uint private trCount; // BUG[PRIVATE] 
 
  mapping(uint => Request) public requests;
  EnumerableSet.UintSet private requestList; // NOTE[SELF]| The index ofrequestList does not necessarily equal the requests index!
  uint private count; // BUG[PRIVATE]

  QuorumRequest public quorumRequest;

  constructor (address[] memory owners, uint q) { // ok
    require(owners.length >= q, 'cunt'); // ok
    for (uint i; i < owners.length; i++) {
      trusted[owners[i]] = true;
      trustedSet.add(owners[i]);
    }
    quorum = q;  // set default quorum
    factory = msg.sender;

  }
  function _confirmQuorum() private trustedOnly {
    require(quorumRequest.m.confirmed == false && quorumRequest.m.initiator != address(0),  'request not raised'); // ok
    require(quorumRequest.m.initiator != msg.sender || block.timestamp > quorumRequest.m.timestamp +  24 * 3600 * 30 || (quorumRequest.m.initiator == msg.sender && quorum==1), 'cunt'); // anyone trusted can modyfy with a 1 out of N quoru or a 30 day timelock. 
    quorumRequest.m.numOfConfirmations += 1;
    if (quorumRequest.m.numOfConfirmations >= quorum || quorumRequest.m.initiator == msg.sender){
      quorumRequest.m.confirmed = true;
      quorum = quorumRequest.quorum;
    }
  } 
  function setQuorum(uint q) external nonReentrant trustedOnly { 
    require(quorumRequest.m.confirmed == true || quorumRequest.m.initiator == address(0), 'request already raised');
    require(q<=trustedSet.length(),  'cunt'); 
    require(q!=quorum,  'cunt'); 
    quorumRequest = QuorumRequest(RequestMetadata(0, msg.sender, false, block.timestamp, 1, 0), q); // creates a QuorumRequest at quorumCount
    if (quorum==1){
      _confirmQuorum();
    }
  }
   function confirmQuorum() external nonReentrant trustedOnly { 
    _confirmQuorum();
  }
  function declineQuorum() external nonReentrant trustedOnly { 
    require(quorumRequest.m.confirmed == false && quorumRequest.m.initiator != address(0),  'request not raised'); // ok
    quorumRequest.m.numOfDeclines += 1;
    if (quorumRequest.m.numOfDeclines > trustedSet.length()-quorum || quorumRequest.m.initiator == msg.sender){
      quorumRequest.m.confirmed = true;
    }
  }
  function confirmT(uint id) private {
    trRequests[id].m.confirmed = true; // ok
    trRequestList.remove(id); // ok
  }
  function _confirmTrusted(uint id) private trustedOnly { // ok // NOTE[PRIV]
    require(trRequests[id].m.confirmed == false && trRequests[id].m.initiator != address(0),  'cunt'); // ok
    require(trRequests[id].m.initiator != msg.sender || block.timestamp > trRequests[id].m.timestamp +  24 * 3600 * 30 || (trRequests[id].m.initiator == msg.sender && quorum==1), 'cunt'); // ok
    trRequests[id].m.numOfConfirmations += 1;
    if (trRequests[id].m.numOfConfirmations >= quorum || trRequests[id].m.initiator == msg.sender){
      confirmT(id);
      trusted[trRequests[id].user] = trRequests[id].isTrusted; // ok // anyone trusted can be added with a 2 out of N quoru or a 30 day timelock. 
      if (trRequests[id].isTrusted){
        trustedSet.add(trRequests[id].user);

      }else { 
        trustedSet.remove(trRequests[id].user);
      }
      FactoryStub(factory).updateOwner(trRequests[id].user, trRequests[id].isTrusted);
      
      }
  }
   function setTrusted(address _user, bool _isTrusted) external nonReentrant trustedOnly  returns(uint){ // NOTE[PRIV] // BUG[VALIDATION]| Lack of validation that _isTrusted is presently not set
    require(_isTrusted == true || (_isTrusted == false && trusted[_user] == true && quorum<=trustedSet.length()-1) ,  'cunt'); 
    if(_isTrusted == false){
      require(quorumRequest.m.confirmed == true || quorumRequest.m.initiator == address(0) || (quorumRequest.m.confirmed == false && quorumRequest.m.initiator != address(0)  && quorumRequest.quorum<=trustedSet.length()-1) , 'max quorum request raised');
    }
    trRequests[trCount] = TrustedRequest(RequestMetadata(trCount, msg.sender, false, block.timestamp, 1, 0), _user, _isTrusted); // creates a trRequest at trCount
    trRequestList.add(trCount); // adds it to trRequestList
    if (quorum==1){
      _confirmTrusted(trCount);
    }
    trCount += 1; // increment count
    return trCount-1; 
  }
  function confirmTrusted(uint id) external nonReentrant trustedOnly { 
    _confirmTrusted(id);
  }
  function declineTrusted(uint id) external nonReentrant trustedOnly { // NOTE[PRIV] // any trusted request can be instantly declined.
    require(trRequests[id].m.confirmed == false && trRequests[id].m.initiator != address(0),  'cunt'); // ok
    trRequests[id].m.numOfDeclines += 1;
    if (trRequests[id].m.numOfDeclines > trustedSet.length()-quorum || trRequests[id].m.initiator == msg.sender){
      confirmT(id);
    }
  }
  function confirmW(uint id) private{
    requests[id].m.confirmed = true; // ok
    requestList.remove(id); // ok
  }
  function _confirmWithdraw(uint id) private trustedOnly { // NOTE[PRIV]
    require(requests[id].m.confirmed == false, 'cunt'); // ok
    require(requests[id].m.initiator != msg.sender || block.timestamp > requests[id].m.timestamp + 24 * 3600 * 30 || (requests[id].m.initiator == msg.sender && quorum==1), 'cunt'); // ok
    requests[id].m.numOfConfirmations += 1;
    if (requests[id].m.numOfConfirmations >= quorum || requests[id].m.initiator == msg.sender){
      confirmW(id);
      if (address(requests[id].token) == address(0)) { // ok
        Address.sendValue(payable(requests[id].m.initiator), requests[id].amount); // ok
      } else { // ok
        requests[id].token.safeTransfer(requests[id].m.initiator, requests[id].amount); // ok
      } // ok
    }
  } // ok

   function initWithdraw(IERC20 token, uint amount) external nonReentrant trustedOnly  returns(uint){ // NOTE[PRIV]
    requests[count] = Request(RequestMetadata(count, msg.sender, false, block.timestamp, 1, 0), amount,token); // ok
    requestList.add(count); // ok
    if (quorum==1){
      _confirmWithdraw(count);
    }
    count += 1; // ok
    return count-1; 
  }
    function confirmWithdraw(uint id) external nonReentrant trustedOnly { 
    _confirmWithdraw(id);
  }

  function declineWithdraw(uint id) external nonReentrant trustedOnly { // NOTE[PRIV]
    require(requests[id].m.confirmed == false && requests[id].m.initiator != address(0),  'cunt'); // ok
    requests[id].m.numOfDeclines += 1;
    if (requests[id].m.numOfDeclines > trustedSet.length()-quorum || requests[id].m.initiator == msg.sender){
    confirmW(id);
    }
  }
  function listRequests() external view returns (Request[] memory) { // BUG[info]| Gas cost
		Request[] memory list = new Request[](requestList.length()); // ok
		for (uint i; i < requestList.length(); i++) { // ok
      list[i] = requests[requestList.at(i)]; // ok
		} // ok
		return list; // ok
	}
  function listTrustedRequests() external view returns (TrustedRequest[] memory) { // BUG[info]| Gas cost
		TrustedRequest[] memory list = new TrustedRequest[](trRequestList.length()); // ok
		for (uint i; i < trRequestList.length(); i++) { // ok
      list[i] = trRequests[trRequestList.at(i)]; // ok
		} // ok
		return list; // ok
	}
  receive() external payable {} // ok // ok
}

